        "activityBar.foreground": "#E0E1E4",            // 活动栏前景色
        "activityBar.inactiveForeground": "#6b6b6d",    // 活动栏非活动状态前景色
        "activityBar.border": "#101011",                // 活动栏边框颜色
        "activityBar.activeBorder": "#65bd9c",          // 活动栏指示器边框颜色
