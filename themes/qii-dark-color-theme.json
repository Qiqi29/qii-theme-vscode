        "badge.foreground": "#E0E1E4",  // 徽章前景色

        // 下拉列表
        "dropdown.background": "#27282C",   // 下拉列表背景色
        "dropdown.foreground": "#E0E1E4",   // 下拉列表前景色
        "dropdown.border": "#101011",       // 下拉列表边框

        // 滚动条
        "scrollbarSlider.background": "#FFFFFF15",          // 滚动条颜色
        "scrollbarSlider.hoverBackground": "#FFFFFF1A",     // 滚动条悬停颜色
        "scrollbarSlider.activeBackground": "#FFFFFF20",    // 滚动条点击颜色
        "editorOverviewRuler.border": "#1C1D20",            // 滚动条边框颜色
        "scrollbar.shadow": "#10101180",                    // 滚动条阴影

        // 小地图
        "minimap.background": "#1C1D20EF",                  // 小地图背景色
        "minimapSlider.background": "#FFFFFF15",            // 小地图滑块颜色
        "minimapSlider.hoverBackground": "#FFFFFF1A",       // 小地图滑块悬停颜色
        "minimapSlider.activeBackground": "#FFFFFF20",      // 小地图滑块点击颜色
        "minimap.findMatchHighlight": "#3da27df0",          // 小地图搜索匹配颜色
        "minimap.selectionHighlight": "#3da27dA0",          // 小地图选中文本背景色

        // 按钮
        "button.background": "#3da27d",         // 按钮背景色
        "button.hoverBackground": "#338668",    // 按钮悬停背景色
        "button.border": "#4bbc93",             // 按钮边框颜色
        "button.foreground": "#FAFAFC",         // 按钮前景色

        // 输入框
        "input.background": "#1E2024",      // 输入框背景色
        "input.foreground": "#E0E1E4",      // 输入框前景色
        "input.border": "#8490aa30",        // 输入框边框色

        // 其他
        "focusBorder": "#FFFFFF00",         // 焦点元素的边框颜色
        "icon.foreground": "#FFFFFFA0",     // 图标的默认颜色

        // 终端代码颜色
        "terminal.foreground": "#b2b4b7",
        "terminal.ansiBlack": "#2d3139",
        "terminal.ansiBlue": "#84b3ff",
        "terminal.ansiGreen": "#9FC57C",
        "terminal.ansiYellow": "#e2b77e",
        "terminal.ansiCyan": "#73BAC2",
        "terminal.ansiMagenta": "#c589e0",
        "terminal.ansiRed": "#f37a94",
        "terminal.ansiWhite": "#d7dae0",
        "terminal.ansiBrightBlack": "#7f848e",
        "terminal.ansiBrightBlue": "#84b3ff",
        "terminal.ansiBrightGreen": "#9FC57C",
        "terminal.ansiBrightYellow": "#e2b77e",
        "terminal.ansiBrightCyan": "#73BAC2",
        "terminal.ansiBrightMagenta": "#c589e0",
        "terminal.ansiBrightRed": "#f37a94",
        "terminal.ansiBrightWhite": "#d7dae0",

        // 括号颜色
        "editorBracketHighlight.foreground1": "#f0d46e",
        "editorBracketHighlight.foreground2": "#9fc37d",
        "editorBracketHighlight.foreground3": "#83c5ff",
        "editorBracketHighlight.foreground4": "#d091ed",
        "editorBracketHighlight.foreground5": "#6dd2cc",

        /// 波浪线
        "editorInfo.foreground": "#83c5ff",
        "editorError.foreground": "#ff7e7e",
        "editorWarning.foreground": "#f0d46e",
