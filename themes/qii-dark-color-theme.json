        "list.inactiveSelectionForeground": "#E0E1E4",  // 列表中已选项非活动前景色
        "list.dropBackground": "#192e26",               // 列表中拖拽背景色
        "list.focusBackground": "#313640",              // 列表中焦点项背景色
