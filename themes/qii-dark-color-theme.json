    "tokenColors": [
        {
            "name": "注释 / 注释的符号",
            "scope": [
                "comment",
                "punctuation.definition.comment",
            ],
            "settings": {
                "foreground": "#929395"
            }
        },
        {
            "name": "字符串 / 模板字符串",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#c2e095"
            }
        },
        {
            "name": "数字",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#e4bc77"
            }
        },
        {
            "name": "数学符号 / Math",
            "scope": "support.constant.math",
            "settings": {
                "foreground": "#e4bc77"
            }
        },
        {
            "name": "内置常数 / true false null..",
            "scope": "constant.language",
            "settings": {
                "foreground": "#63c0cd"
            }
        },
        {
            "name": "用户定义常量 / CSS颜色 正则",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#63c0cd"
            }
        },
        {
            "name": "转义字符 / \n \t",
            "scope": [
                "constant.character.escape",
            ],
            "settings": {
                "foreground": "#e4bc77"
            }
        },
        {
            "name": "正则表达式",
            "scope": [
                "string.regexp", 
            ],
            "settings": {
                "foreground": "#c2e095"
            }
        },
        {
            "name": "正则表达式匹配符 / i g",
            "scope": [
                "string.regexp keyword.other"
            ],
            "settings": {
                "foreground": "#f37a94"
            }
        },
        {
            "name": "模板字符串符号 / ${..}",
            "scope": [
                "string.embedded.begin",
                "string.embedded.end",
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#63c0cd"
            }
        },
        {
            "name": "嵌入式代码符号 / . ? :",
            "scope": [
                "punctuation.section.embedded.begin.js",
                "punctuation.section.embedded.end.js",
                "punctuation.section.embedded.begin.erb",
                "punctuation.section.embedded.end.erb",
                "source.elixir.embedded",
                "punctuation.separator",
                "punctuation.accessor",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#989ca6"
            }
        },
        {
            "name": "类型 / const let function class",
            "scope": "storage.type",
            "settings": {
                "foreground": "#63c0cd"
            }
        },
        {
            "name": "语言变量 / this super",
            "scope": "variable.language",
            "settings": {
                "foreground": "#f37a94"
            }
        },
        {
            "name": "关键词 / if for return import export try await",
            "scope": [
                "keyword",
                "keyword.operator.logical",
                "keyword.operator.constructor"
            ],
            "settings": {
                "foreground": "#f37a94"
            }
        },
        {
            "name": "运算符 / + - * == typeof ...",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#f37a94"
            }
        },
        {
            "name": "存储 / extends",
            "scope": "storage",
            "settings": {
                "foreground": "#f37a94"
            }
        },
        {
            "name": "类名 / Class Promise Object",
            "scope": [
                "entity.name.class",
                "entity.name.module",
                "entity.name.type",
                "storage.identifier",
                "support.class"
            ],
            "settings": {
                "foreground": "#e4bc77"
            }
        },
        {
            "name": "继承的类",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#9bca9e"
            }
        },
        {
            "name": "定义常量",
            "scope": [
                "variable.other.constant",
            ],
            "settings": {
                "foreground": "#84b3ff"
            }
        },
        {
            "name": "变量对象",
            "scope": [
                "variable.other.object",
                "variable.other.global",
                "variable.other.readwrite.class",
                "variable.other.readwrite.instance",
                "variable.other.readwrite.batchfile",
                "variable.readwrite",
                "variable.readwrite.other.block"
            ],
            "settings": {
                "foreground": "#C6C7CD"
            }
        },
        {
            "name": "其他变量",
            "scope": [
                "variable.other",
                "variable.other.property",
                "variable.other.block"
            ],
            "settings": {
                "foreground": "#C6C7CD"
            }
        },
        {
            "name": "其他变量",
            "scope": [
                "variable.other.object.property"
            ],
            "settings": {
                "foreground": "#6ebff2"
            }
        },
        {
            "name": "包导入 java",
            "scope": [
                "storage.modifier.import",
                "storage.modifier.package"
            ],
            "settings": {
                "foreground": "#84b3ff"
            }
        },
        {
            "name": "内置函数",
            "scope": [
                "function.support.builtin",
                "function.support.core"
            ],
            "settings": {
                "foreground": "#9bca9e"
            }
        },
        {
            "name": "函数名 / CSS函数",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#84b3ff"
            }
        },
        {
            "name": "函数参数",
            "scope": [
                "variable.parameter",
                "entity.name.variable.parameter",
                "parameter.variable"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#C6C7CD"
            }
        },
        {
            "name": "html 标签名称",
            "scope": [
                "entity.name.tag",
                "entity.name.tag.class.js"
            ],
            "settings": {
                "foreground": "#78c2f4"
