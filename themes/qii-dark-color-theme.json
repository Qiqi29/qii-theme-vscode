		"scrollbarSlider.background": "#4a4e5760",           // 滚动条颜色
		"scrollbarSlider.hoverBackground": "#4c505680",      // 滚动条悬停颜色
		"scrollbarSlider.activeBackground": "#53565daa",     // 滚动条点击颜色
		"editorOverviewRuler.border": "#1b1c20",             // 滚动条边框颜色
    // 其他
		"focusBorder": "#FFFFFF00",                          // 焦点元素的边框颜色
		"icon.foreground": "#FFFFFF90",                      // 图标的默认颜色
		// 按钮
		"button.background": "#41ab84",                      // 按钮背景色
		"button.foreground": "#101214",                      // 按钮前景色
		"button.hoverBackground": "#399a77",                 // 按钮悬停背景色
		// 输入框
		"input.background": "#1b1c20",                       // 输入框背景
		"input.border": "#25262A",                           // 输入框边框
		// 列表
		"list.focusBackground": "#212328",                   // 列表中焦点项背景色
		"list.focusForeground": "#d7dae0",                   // 列表中焦点项前景色
		"list.focusOutline": "#494c56",                      // 列表中焦点项边框色
		"list.hoverBackground": "#1d1f23",                   // 列表中鼠标悬停背景色
		"list.hoverForeground": "#d7dae0",                   // 列表中鼠标悬停前景色
		"list.activeSelectionBackground": "#212328",         // 列表中已选项背景色
		"list.activeSelectionForeground": "#d7dae0",         // 列表中已选项前景色
		"list.inactiveSelectionBackground": "#212328",       // 列表中已选项非活动背景色
		"list.inactiveSelectionForeground": "#d7dae0",       // 列表中已选项非活动前景色
		"list.highlightForeground": "#C5C5C5",               // 列表中搜索匹配高亮色
    // 下拉列表
		"dropdown.background": "#1d1e20",                    // 下拉列表背景色
		"dropdown.foreground": "#aaabae",                    // 下拉列表前景色
		"dropdown.border": "#222226",                        // 下拉列表边框
		// 活动栏
		"activityBar.background": "#1b1c20",                 // 活动栏背景色
		"activityBar.foreground": "#e6e7ec",                 // 活动栏前景色
		"activityBar.border": "#232428",                     // 活动栏边框颜色
		"activityBar.inactiveForeground": "#7e7f84",         // 活动栏非活动状态前景色
		"activityBar.activeBorder": "#65bd9c",               // 活动栏指示器边框颜色
		"activityBarBadge.background": "#41ab84",            // 活动栏通知徽标背景色
		"activityBarBadge.foreground": "#101214",            // 活动栏通知徽标前景色
		// 侧边栏
		"sideBar.background": "#16171a",                     // 侧边栏背景色
		"sideBar.border": "#222226",                         // 侧边栏边框颜色
		"sideBarSectionHeader.background": "#1b1c20",        // 侧边栏分组标题背景色
		// 迷你地图
		"minimap.background" : "#1b1c20a0",                  // 小地图背景色
		// 底部状态栏
		"statusBar.background": "#1b1c20",                   // 状态栏背景色
		"statusBar.foreground": "#7e7f84",                   // 状态栏前景色
		"statusBar.border": "#222226",                       // 状态栏边框颜色
		"statusBar.debuggingBackground": "#41ab84",          // 程序调试时背景色
		"statusBar.debuggingForeground": "#101214",          // 程序调试时前景色
    "statusBar.noFolderBackground": "#1b1c20",           // 未打开文件夹背景色 
    "statusBar.noFolderForeground": "#74757b",           // 未打开文件夹前景色
		// 菜单栏
		"menu.background": "#101011",                        // 菜单栏背景色
		"menu.foreground": "#7e7f84",                        // 菜单栏前景色
		"menu.border": "#1c1e21",                            // 菜单栏边框颜色
		"menu.selectionBackground": "#232428",               // 菜单栏选中项背景色
		"menu.separatorBackground": "#1c1e21",               // 菜单栏中分隔线颜色
    // 底部面板
		"panel.background" : "#16171a",                      // 面板背景颜色
		"panel.border" : "#222226",                          // 面板边框颜色
		"badge.background": "#2e2f31",                       // 徽章背景色
    "badge.foreground": "#b2b4b7",                       // 徽章前景色
    // 终端代码颜色
    "terminal.foreground": "#b2b4b7",
    "terminal.ansiBlack": "#2d3139",
    "terminal.ansiBlue": "#85AAE5",
    "terminal.ansiGreen": "#9FC57C",
    "terminal.ansiYellow": "#e0be6a",
    "terminal.ansiCyan": "#73BAC2",
    "terminal.ansiMagenta": "#c589e0",
    "terminal.ansiRed": "#e06a86",

    "terminal.ansiWhite": "#d7dae0",
    "terminal.ansiBrightBlack": "#7f848e",
    "terminal.ansiBrightBlue": "#85AAE5",
    "terminal.ansiBrightGreen": "#9FC57C",
    "terminal.ansiBrightYellow": "#e0be6a",
    "terminal.ansiBrightCyan": "#73BAC2",
    "terminal.ansiBrightMagenta": "#c589e0",
    "terminal.ansiBrightRed": "#e06a86",
    "terminal.ansiBrightWhite": "#d7dae0",
    // 括号颜色
    "editorBracketHighlight.foreground1": "#dab159",
    "editorBracketHighlight.foreground2": "#73BAC2",
    "editorBracketHighlight.foreground3": "#85AAE5",
    "editorBracketHighlight.foreground4": "#c589e0",
    /// 波浪线
    "editorInfo.foreground": "#6b99df",
    "editorError.foreground": "#ff7e7e",
    "editorWarning.foreground": "#f3da88",
	},
	"tokenColors": [
		{
			"name": "注释",
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": {
				"foreground": "#87878c"
			}
		},
		{
      "name": "字符串",
      "scope": [
        "string",
        "string.template"
      ],
      "settings": {
        "foreground": "#e0b16a"
      }
    },
    {
      "name": "数字",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#9FC57C"
      }
    },
    {
      "name": "嵌入式字符串 开始/结束",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#9FC57C"
      }
    },
    {
      "name": "默认嵌入颜色 .?:",
      "scope": [
        "punctuation.section.embedded.begin.js",
        "punctuation.section.embedded.end.js",
        "punctuation.section.embedded.begin.erb",
        "punctuation.section.embedded.end.erb",
        "source.elixir.embedded",
        "punctuation.separator",
        "punctuation.accessor",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#828283"
      }
    },
    {
      "name": "类型 const/let",
      "scope": "storage.type",
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "内置常数  true/false",
      "scope": "constant.language",
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "自定义常数  CSS颜色代码/正则",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "语言变量  this/super",
      "scope": "variable.language",
      "settings": {
        "foreground": "#e06a86"
      }
    },
    {
      "name": "关键词 return/import/export",
      "scope": [
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#e06a86"
      }
    },
    {
      "name": "关键词 运算符 + - * / ...",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#e06a86"
      }
    },
    {
      "name": "存储 extends",
      "scope": "storage",
      "settings": {
        "foreground": "#e06a86"
      }
    },
    {
      "name": "类 Promise/类名/...",
      "scope": [
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#85AAE5"
      }
    },
    {
      "name": "变量对象",
      "scope": [
        "variable.other.object",
        "variable.other.constant",
        "variable.other.global",
        "variable.other.readwrite.class",
        "variable.other.readwrite.instance",
        "variable.other.readwrite.batchfile",
        "variable.readwrite",
        "variable.readwrite.other.block"
      ],
      "settings": {
        "foreground": "#85AAE5"
      }
    },
    {
      "name": "其他变量",
      "scope": [
        "variable.other",
        "variable.other.property",
        "variable.other.block"
      ],
      "settings": {
        "foreground": "#BDBEC2"
      }
    },
    {
      "name": "继承类",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#9FC57C"
      }
    },
    {
      "name": "包导入 java",
      "scope": [
        "storage.modifier.import",
        "storage.modifier.package"
      ],
      "settings": {
        "foreground": "#75A3EE"
      }
    },
    {
      "name": "内置函数",
      "scope": [
        "function.support.builtin",
        "function.support.core"
      ],
      "settings": {
        "foreground": "#acc382"
      }
    },
    {
      "name": "函数名",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#acc382"
      }
    },
    {
      "name": "函数参数",
      "scope": [
        "variable.parameter",
        "entity.name.variable.parameter",
        "parameter.variable"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c09ada"
      }
    },
    {
      "name": "函数调用",
      "scope": "entity.name.function-call",
      "settings": {
        "foreground": "#bfadab"
      }
    },
    {
      "name": "html 标签名称",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.class.js"
      ],
      "settings": {
        "foreground": "#e06a86"
      }
    },
    {
      "name": "HTML 标签括号",
      "scope": [
        "punctuation.definition.tag",
      ],
      "settings": {
        "foreground": "#999ba4"
      }
    },
    {
      "name": "html 标签类别和ID",
      "scope": [
        "entity.name.tag.class",
        "entity.name.tag.id"
      ],
      "settings": {
        "foreground": "#85AAE5"
      }
    },
    {
      "name": "html 标签属性",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#acc382"
      }
    },
    {
      "name": "库常数 JSON/",
      "scope": "support.constant",
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "库 类/类型",
      "scope": [
        "support.type",
        "support.variable"
      ],
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "Json",
      "scope": "support.dictionary.json",
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "样式表属性名称",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.scss",
        "support.type.property-name.less",
        "support.type.property-name.sass"
      ],
      "settings": {
        "foreground": "#BDBEC2"
      }
    },
    {
      "name": "CSS伪属性名称",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-class.scss",
        "entity.other.attribute-name.pseudo-class.less",
        "entity.other.attribute-name.pseudo-class.sass",
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-element.scss",
        "entity.other.attribute-name.pseudo-element.less",
        "entity.other.attribute-name.pseudo-element.sass"
      ],
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "CSS样式表属性值",
      "scope": [
        "support.constant.css",
        "support.constant.scss",
        "support.constant.less",
        "support.constant.sass"
      ],
      "settings": {
        "foreground": "#acc382"
      }
    },
    {
      "name": "CSS样式表变量",
      "scope": [
        "variable.css",
        "variable.scss",
        "variable.less",
        "variable.sass"
      ],
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "CSS样式表变量字符串",
      "scope": [
        "variable.css.string",
        "variable.scss.string",
        "variable.less.string",
        "variable.sass.string"
      ],
      "settings": {
        "foreground": "#e0b16a"
      }
    },
    {
      "name": "样式表单元",
      "scope": [
        "unit.css",
        "unit.scss",
        "unit.less",
        "unit.sass"
      ],
      "settings": {
        "foreground": "#e06a86"
      }
    },
    {
      "name": "样式表函数",
      "scope": [
        "function.css",
        "function.scss",
        "function.less",
        "function.sass"
      ],
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "库变量",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "无效",
      "scope": "invalid",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "无效已弃用",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#F8F8F0"
      }
    },
    {
      "name": "JSON 字符串",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "链接",
      "scope": "string.detected-link",
      "settings": {
        "foreground": "#85AAE5"
      }
    },
    {
      "name": "diff.header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#e06a86"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#acc382"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#e0b16a"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#56b6c2A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#e0b16a"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic, markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Punctuation",
      "scope": [
        "punctuation.definition.italic.markdown",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#676f7d"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#85AAE5"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e06a86"
      }
    },
    {
      "name": "Markup: Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "foreground": "#acc382"
      }
    },
    {
      "name": "Markup: Separator",
      "scope": "meta.separator.markdown",
      "settings": {
        "foreground": "#e06a86",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Raw",
      "scope": [
        "markup.raw.inline.markdown",
        "markup.raw.block.markdown"
      ],
      "settings": {
        "foreground": "#73BAC2"
      }
    },
    {
      "name": "Markup: List Punctuation",
      "scope": "punctuation.definition.list_item.markdown",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffffff"
      }
    }
	]
}
