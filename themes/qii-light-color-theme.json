{
    "name": "Qii Theme Light",
    "type": "light",
    "colors": {
        // 基本
        "foreground": "#5D5F62",            // 整体前景色
        "disabledForeground": "#b5b7bd",    // 禁用状态前景色
        "focusBorder": "#65bd9c",           // 焦点元素边框颜色
        "icon.foreground": "#5D5F62cd",     // 图标的默认颜色

        // 编辑器
        "editor.background": "#FAFAFA",                     // 编辑器背景色
        "editorGroup.emptyBackground": "#FAFAFA",           // 空编辑器背景色
        "editor.foreground": "#5D5F62",                     // 编辑器前景色
        "editorGroup.border": "#32456514",                  // 编辑器分割线颜色

        "editorGroup.dropBackground": "#75d3b150",          // 拖动时背景色
        "editorCursor.foreground": "#65bd9c",               // 光标颜色
        "editorWhitespace.foreground": "#c4c9cc",           // 空白字符颜色
        "editorIndentGuide.background1": "#3245651A",       // 缩进参考线颜色
        "editorLineNumber.foreground": "#bcc0c4",           // 行号颜色
        "editorLineNumber.activeForeground": "#2f3032",     // 当前行号颜色
        "editor.selectionBackground": "#d7eae4",            // 选中内容背景色
        "editor.lineHighlightBackground": "#F0F0F0",        // 光标所在行高亮色
        "editor.selectionHighlightBackground": "#00000015", // 与所选内容相同的背景色
        "editor.selectionHighlightBorder": "#00000020",     // 与所选内容相同的边框色
        "editor.wordHighlightTextBackground": "#00000020",  // 字符搜索匹配背景色
        "editor.hoverHighlightBackground": "#00000015",     // 突出悬停的字词背景色
        "editor.wordHighlightBackground": "#00000015",      // 单词突出显示背景色
        "editor.wordHighlightBorder": "#00000020",          // 单词突出显示边框色
        "editorHoverWidget.background": "#EAEAEB",          // 悬停提示背景色
        "editorHoverWidget.border": "#3245652A",            // 悬停提示边框色
        "editorWidget.background": "#EAEAEB",               // 组件/搜索菜单背景色
        "editorSuggestWidget.background": "#EAEAEB",        // 建议组件背景色
        "editor.foldBackground": "#ebebebcc",               // 折叠行背景色
        "sash.hoverBorder": "#65bd9cCC",                    // 拖动编辑器的反馈颜色

        // 标题栏
        "titleBar.activeBackground": "#EAEAEB",     // 背景色
        "titleBar.activeForeground": "#5D5F62",     // 前景色
        "titleBar.inactiveBackground": "#EAEAEB",   // 非活动背景色
        "titleBar.inactiveForeground": "#5D5F6280", // 非活动前景色
        "titleBar.border": "#FFFFFF00",             // 分割线颜色

        // 选项卡列表
        "editorGroupHeader.tabsBackground": "#FAFAFA",  // 背景色
        "editorGroupHeader.tabsBorder": "#FAFAFA",      // 分割线颜色
        "tab.hoverBackground": "#EAEAEB",               // 鼠标悬停背景色
        "tab.inactiveBackground": "#FFFFFF00",          // 未选中背景色
        "tab.activeBackground": "#e2f6e6",              // 选中背景色
        "tab.border": "#FFFFFF00",                      // 左右边框颜色
        "tab.activeBorder": "#acdcb7",                  // 选中状态下边框颜色

        // 列表和树
        "list.activeSelectionBackground": "#1c293c14",  // 已选项背景色
        "list.inactiveSelectionBackground": "#1c293c14",// 已选项非活动背景色
        "list.activeSelectionForeground": "#101012",    // 已选项前景色
        "list.inactiveSelectionForeground": "#101012",  // 已选项非活动前景色
        "list.dropBackground": "#1c293c14",             // 拖拽背景色
        "list.focusBackground": "#1c293c14",            // 焦点项背景色
        "list.focusForeground": "#FFFFFF",              // 焦点项前景色
        "list.focusOutline": "#65bd9c",                 // 焦点项边框色
        "list.hoverBackground": "#1c293c14",            // 鼠标悬停背景色
        "list.hoverForeground": "#101012",              // 鼠标悬停前景色
        "list.highlightForeground": "#101012",          // 搜索匹配高亮色
        "tree.indentGuidesStroke": "#FFFFFF00",         // 缩进线颜色

        // 左侧活动栏
        "activityBar.background": "#EAEAEB",            // 活动栏背景色
        "activityBar.activeBackground": "#EAEAEB",      // 活动栏选中背景色
        "activityBar.foreground": "#353536",            // 活动栏前景色
        "activityBar.inactiveForeground": "#a3a4a7",    // 活动栏非活动状态前景色
        "activityBar.border": "#32456514",              // 活动栏边框颜色
        "activityBar.activeBorder": "#65bd9c",          // 活动栏指示器边框颜色
        "activityBarBadge.background": "#65bd9c",       // 活动栏通知徽标背景色
        "activityBarBadge.foreground": "#ffffff",       // 活动栏通知徽标前景色

        // 左侧菜单（资源管理器/搜索/..）
        "sideBar.background": "#f3f3f4",                // 背景色
        "sideBar.border": "#32456514",                  // 边框颜色
        "sideBarSectionHeader.background": "#FAFAFA",   // 分组标题背景色

        // 菜单
        "menu.background": "#FFFFFF",             // 菜单背景色
        "menu.foreground": "#5D5F62",             // 菜单前景色
        "menu.border": "#32456514",               // 菜单边框颜色
        "menu.selectionBackground": "#d8f2e2",    // 菜单选中项背景色
        "menu.selectionForeground": "#101012",    // 菜单选中项前景色
        "menu.separatorBackground": "#32456514",  // 菜单分隔线颜色

        // 底部状态栏
        "statusBar.background": "#EAEAEB",              // 状态栏背景色
        "statusBar.foreground": "#636569",              // 状态栏前景色
        "statusBar.border": "#FFFFFF00",                // 状态栏边框颜色
        "statusBar.debuggingBackground": "#e2f6e6",     // 程序调试时背景色
        "statusBar.debuggingForeground": "#303032",     // 程序调试时前景色
        "statusBar.noFolderBackground": "#EAEAEB",      // 未打开文件夹背景色
        "statusBar.noFolderForeground": "#636569",      // 未打开文件夹前景色

        // 底部面板
        "panel.background": "#f3f3f4",  // 面板背景颜色
        "panel.border": "#32456514",    // 面板边框颜色
        "badge.background": "#65bd9c",  // 徽章背景色
        "badge.foreground": "#FFFFFF",  // 徽章前景色

        // 下拉列表
        "dropdown.background": "#FFFFFF",   // 下拉列表背景色
        "dropdown.foreground": "#5D5F62",   // 下拉列表前景色
        "dropdown.border": "#32456514",     // 下拉列表边框

        // 滚动条
        "scrollbarSlider.background": "#272d3820",          // 滚动条颜色
        "scrollbarSlider.hoverBackground": "#272d382F",     // 滚动条悬停颜色
        "scrollbarSlider.activeBackground": "#272d3835",    // 滚动条点击颜色
        "editorOverviewRuler.border": "#E0E3E4",            // 滚动条边框颜色
        "scrollbar.shadow": "#1010111f",                    // 滚动条阴影

        // 小地图
        "minimap.background": "#FAFAFAEF",                  // 小地图背景色
        "minimapSlider.background": "#272d382A",            // 小地图滑块颜色
        "minimapSlider.hoverBackground": "#272d382F",       // 小地图滑块悬停颜色
        "minimapSlider.activeBackground": "#272d3835",      // 小地图滑块点击颜色
        "minimap.findMatchHighlight": "#3da27df0",          // 小地图搜索匹配颜色
        "minimap.selectionHighlight": "#3da27da0",          // 小地图选中文本背景色

        // 按钮
        "button.background": "#65bd9c",         // 按钮背景色
        "button.hoverBackground": "#5bae8f",    // 按钮悬停背景色
        "button.border": "#5bae8f",             // 按钮边框颜色
        "button.foreground": "#FFFFFF",         // 按钮前景色

        // 输入框
        "input.background": "#e3e4e4",      // 输入框背景色
        "input.foreground": "#5D5F62",      // 输入框前景色
        "input.border": "#32456514",        // 输入框边框色

        // 终端代码颜色
        "terminal.foreground": "#5D5F62",
        "terminal.ansiBlack": "#2d3139",
        "terminal.ansiBlue": "#2f86f0",
        "terminal.ansiGreen": "#58AA46",
        "terminal.ansiYellow": "#E0B16A",
        "terminal.ansiCyan": "#24B0B0",
        "terminal.ansiMagenta": "#c82fbb",
        "terminal.ansiRed": "#E1496A",
        "terminal.ansiWhite": "#5D5F62",
        "terminal.ansiBrightBlack": "#7f848e",
        "terminal.ansiBrightBlue": "#2f86f0",
        "terminal.ansiBrightGreen": "#58AA46",
        "terminal.ansiBrightYellow": "#E0B16A",
        "terminal.ansiBrightCyan": "#24B0B0",
        "terminal.ansiBrightMagenta": "#c82fbb",
        "terminal.ansiBrightRed": "#E1496A",
        "terminal.ansiBrightWhite": "#5D5F62",

        // 括号颜色
        "editorBracketHighlight.foreground1": "#1b81e8",
        "editorBracketHighlight.foreground2": "#c16926",
        "editorBracketHighlight.foreground3": "#63a900",
        "editorBracketHighlight.foreground4": "#0da59b",
        "editorBracketHighlight.foreground5": "#b89102",
        "editorBracketHighlight.foreground6": "#ac51d6",

        // 波浪线
        "editorInfo.foreground": "#3595f5",
        "editorError.foreground": "#ff4242",
        "editorWarning.foreground": "#f0c732",
    },
    "tokenColors": [
        {
            "name": "注释 / 注释符号",
            "scope": [
                "comment",
                "punctuation.definition.comment",
            ],
            "settings": {
                "foreground": "#a2a8ad",
            }
        },
        {
            "name": "字符串 / 模板字符串",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#58AA46"
            }
        },
        {
            "name": "数字",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#24b0b0"
            }
        },
        {
            "name": "数学符号 / Math",
            "scope": "support.constant.math",
            "settings": {
                "foreground": "#24B0B0"
            }
        },
        {
            "name": "类型 / const let function class",
            "scope": "storage.type",
            "settings": {
                "foreground": "#24B0B0"
            }
        },
        {
            "name": "内置常数 / true false null..",
            "scope": "constant.language",
            "settings": {
                "foreground": "#24B0B0"
            }
        },
        {
            "name": "关键词 / if for return import export try await",
            "scope": [
                "keyword",
                "keyword.operator.constructor"
            ],
            "settings": {
                "foreground": "#c82fbb"
            }
        },
        {
            "name": "扩展 / async extends",
            "scope": "storage",
            "settings": {
                "foreground": "#c82fbb"
            }
        },
        {
            "name": "与或非 / && || !",
            "scope": [
                "keyword.operator.logical",
            ],
            "settings": {
                "foreground": "#E1496A"
            }
        },
        {
            "name": "语言变量 / this super",
            "scope": "variable.language",
            "settings": {
                "foreground": "#E1496A"
            }
        },
        {
            "name": "运算符 / + - * == typeof new",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#e1496a"
            }
        },
        {
            "name": "类 名称 / Class",
            "scope": [
                "entity.name.type",
            ],
            "settings": {
                "foreground": "#D77A00"
            }
        },
        {
            "name": "继承的类",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#D77A00"
            }
        },
        {
            "name": "内置类名 / Promise Object",
            "scope": [
                "entity.name.class",
                "entity.name.module",
                "storage.identifier",
                "support.class"
            ],
            "settings": {
                "foreground": "#D77A00"
            }
        },
        {
            "name": "定义常量",
            "scope": [
                "variable.other.constant",
            ],
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "调用常量",
            "scope": [
                "variable.other.object",
                "variable.other.global",
                "variable.other.readwrite.class",
                "variable.other.readwrite.instance",
                "variable.other.readwrite.batchfile",
                "variable.readwrite",
                "variable.readwrite.other.block"
            ],
            "settings": {
                "foreground": "#5D5F62"
            }
        },
        {
            "name": "其他变量",
            "scope": [
                "variable.other",
            ],
            "settings": {
                "foreground": "#5D5F62"
            }
        },
        {
            "name": "其他变量",
            "scope": [
                "variable.other.property",
                "variable.other.block"
            ],
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "其他变量",
            "scope": [
                "variable.other.object.property"
            ],
            "settings": {
                "foreground": "#5D5F62"
            }
        },
        {
            "name": "用户定义常量 / CSS颜色 正则",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#24B0B0"
            }
        },
        {
            "name": "转义字符 / \n \t",
            "scope": [
                "constant.character.escape",
            ],
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "正则表达式",
            "scope": [
                "string.regexp", 
            ],
            "settings": {
                "foreground": "#58AA46"
            }
        },
        {
            "name": "正则表达式匹配符 / i g",
            "scope": [
                "string.regexp keyword.other"
            ],
            "settings": {
                "foreground": "#E1496A"
            }
        },
        {
            "name": "模板字符串符号 / ${..}",
            "scope": [
                "string.embedded.begin",
                "string.embedded.end",
                "punctuation.definition.template-expression",
                "punctuation.section.embedded"
            ],
            "settings": {
                "foreground": "#24B0B0"
            }
        },
        {
            "name": "嵌入式代码符号 / . ? :",
            "scope": [
                "punctuation.section.embedded.begin.js",
                "punctuation.section.embedded.end.js",
                "punctuation.section.embedded.begin.erb",
                "punctuation.section.embedded.end.erb",
                "source.elixir.embedded",
                "punctuation.separator",
                "punctuation.accessor",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#afb2bb"
            }
        },
        {
            "name": "包导入 java",
            "scope": [
                "storage.modifier.import",
                "storage.modifier.package"
            ],
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "内置函数",
            "scope": [
                "function.support.builtin",
                "function.support.core"
            ],
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "函数名 / CSS函数",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#D77A00"
            }
        },
        {
            "name": "函数参数",
            "scope": [
                "variable.parameter",
                "entity.name.variable.parameter",
                "parameter.variable"
            ],
            "settings": {
                "foreground": "#5D5F62",
                "fontStyle": "italic"
            }
        },
        {
            "name": "对象键",
            "scope": [
                "meta.tag" 
            ],
            "settings": {
                "foreground": "#5D5F62"
            }
        },
        {
            "name": "对象字面量键",
            "scope": [
                "meta.object-literal.key.js",
                "meta.object-literal.key.ts"
            ],
            "settings": {
                "foreground": "#cd6647"
            }
        },
        {
            "name": "html 标签名称",
            "scope": [
                "entity.name.tag",
                "entity.name.tag.class.js"
            ],
            "settings": {
                "foreground": "#1c90f5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "html 标签属性 / CSS 选择器",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#8258ec"
            }
        },
        {
            "name": "HTML 标签括号",
            "scope": [
                "punctuation.definition.tag", 
            ],
            "settings": {
                "foreground": "#afb2bb"
            }
        },
        {
            "name": "CSS属性名称",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.scss",
                "support.type.property-name.less",
                "support.type.property-name.sass"
            ],
            "settings": {
                "foreground": "#5D5F62"
            }
        },
        {
            "name": "CSS伪属性名称",
            "scope": [
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-class.scss",
                "entity.other.attribute-name.pseudo-class.less",
                "entity.other.attribute-name.pseudo-class.sass",
                "entity.other.attribute-name.pseudo-element.css",
                "entity.other.attribute-name.pseudo-element.scss",
                "entity.other.attribute-name.pseudo-element.less",
                "entity.other.attribute-name.pseudo-element.sass"
            ],
            "settings": {
                "foreground": "#24B0B0"
            }
        },
        {
            "name": "CSS属性值",
            "scope": [
                "support.constant.css",
                "support.constant.scss",
                "support.constant.less",
                "support.constant.sass"
            ],
            "settings": {
                "foreground": "#58AA46"
            }
        },
        {
            "name": "CSS变量",
            "scope": [
                "variable.css",
                "variable.scss",
                "variable.less",
                "variable.sass"
            ],
            "settings": {
                "foreground": "#58AA46"
            }
        },
        {
            "name": "CSS变量字符串",
            "scope": [
                "variable.css.string",
                "variable.scss.string",
                "variable.less.string",
                "variable.sass.string"
            ],
            "settings": {
                "foreground": "#E0B16A"
            }
        },
        {
            "name": "CSS数值单位 / rpx",
            "scope": [
                "meta.property-value.css",
            ],
            "settings": {
                "foreground": "#E394DC"
            }
        },
        
        {
            "name": "JSON 键",
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "库常数 JSON",
            "scope": "support.constant",
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "库 类/类型",
            "scope": [
                "support.type",
                "support.variable"
            ],
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "Json 字段",
            "scope": "support.dictionary.json",
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "库变量",
            "scope": "support.other.variable",
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "无效",
            "scope": "invalid",
            "settings": {
                "foreground": "#5D5F62"
            }
        },
        {
            "name": "无效已弃用",
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#5D5F62"
            }
        },
        {
            "name": "链接",
            "scope": "string.detected-link",
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "差异: 标题",
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "差异: 删除",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#E1496A"
            }
        },
        {
            "name": "差异: 插入",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#58AA46"
            }
        },
        {
            "name": "差异: 变化",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#E0B16A"
            }
        },
        {
            "name": "Markdown: 标点符号 / #",
            "scope": [
                "punctuation.definition.italic.markdown",
                "punctuation.definition.bold.markdown",
                "punctuation.definition.heading.markdown"
            ],
            "settings": {
                "foreground": "#676f7d"
            }
        },
        {
            "name": "Markdown: 强调标点符号",
            "scope": "punctuation.definition.italic.markdown",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown: 链接",
            "scope": "markup.underline.link.markdown",
            "settings": {
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "Markdown: 加粗",
            "scope": "markup.bold.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown: 标题",
            "scope": "markup.heading.markdown",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#2f86f0"
            }
        },
        {
            "name": "Markdown: 引用",
            "scope": "markup.quote.markdown",
